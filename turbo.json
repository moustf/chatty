{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/server.js",
        "**/dist/**"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "lint": {},
    "lint:fix": {},
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "test-report.html"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
