{
  "name": "chatty",
  "version": "1.0.0",
  "description": "A chat application for contacting your friends and family.",
  "main": "index.js",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moustf/chatty.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles --runInBand server/__test__/",
    "build": "tsc",
    "lint": "eslint --ext .ts server/",
    "lint-fix": "eslint --ext .ts server/ --fix && cd client && npm run lint-fix",
    "lint:client": "cd client && npm run lint",
    "format-client": "cd client && npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongo-seeding": "^3.7.2",
    "mongoose": "^7.0.0",
    "morgan": "^1.10.0",
    "socket.io": "^4.6.1",
    "supertest": "^6.3.3",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
